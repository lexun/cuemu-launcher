{
  "name": "cuemu-launchpad",
  "version": "0.0.1",
  "description": "Patcher and Launcher for SWG CUEmu",
  "main": "build/main.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --config webpack/dist.js --progress --colors",
    "build:watch": "./node_modules/.bin/webpack --config webpack/dev.js --progress --colors --watch",
    "clean": "./node_modules/.bin/rimraf build",
    "dev": "./node_modules/.bin/concurrent --kill-others \"npm run build:watch\" \"npm run hot\"",
    "hot": "./node_modules/.bin/webpack-dev-server --config webpack/dev.js --hot --progress --colors --port 2992 --inline",
    "lint": "./node_modules/.bin/eslint src",
    "start": "cd build && ../node_modules/.bin/electron .",
    "test": "./node_modules/.bin/mocha --compilers js:babel/register --recursive src/**/*-test.js",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Luke Barbuto",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-core": "^5.8.33",
    "babel-eslint": "^4.1.4",
    "babel-loader": "^5.3.3",
    "bootswatch": "^3.3.5",
    "chai": "^3.4.1",
    "concurrently": "^0.1.1",
    "electron-prebuilt": "^0.34.3",
    "eslint": "^1.9.0",
    "eslint-plugin-react": "^3.8.0",
    "file-loader": "^0.8.4",
    "jsdom": "^7.0.2",
    "mocha": "^2.3.3",
    "mocha-jsdom": "^1.0.0",
    "radium": "^0.14.3",
    "react": "^0.14.2",
    "react-addons-test-utils": "^0.14.2",
    "react-bootstrap": "^0.27.3",
    "react-dom": "^0.14.2",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "redux-thunk": "^1.0.0",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.3",
    "webpack-target-electron-renderer": "^0.1.1"
  },
  "babel": {
    "stage": 0
  }
}
