{
  "name": "cuemu-launchpad",
  "version": "0.0.1",
  "description": "Patcher and Launcher for SWG CUEmu",
  "main": "build/main.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "build:watch": "./node_modules/.bin/webpack --progress --colors --watch",
    "clean": "./node_modules/.bin/rimraf build",
    "dev": "./node_modules/.bin/concurrent --kill-others \"npm run build:watch\" \"npm run hot\"",
    "hot": "./node_modules/.bin/webpack-dev-server --config webpack.config.js --hot --progress --colors --port 2992 --inline",
    "lint": "./node_modules/.bin/eslint src",
    "start": "cd build && ../node_modules/.bin/electron .",
    "test": "./node_modules/.bin/mocha --compilers js:babel/register --recursive src/**/*-test.js",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Luke Barbuto",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "bootswatch": "^3.3.5",
    "chai": "^3.3.0",
    "concurrently": "^0.1.1",
    "electron-prebuilt": "^0.33.7",
    "eslint": "^1.6.0",
    "eslint-plugin-react": "^3.5.1",
    "file-loader": "^0.8.4",
    "jsdom": "^6.5.1",
    "mocha": "^2.3.3",
    "mocha-jsdom": "^1.0.0",
    "radium": "^0.14.1",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.0",
    "react-bootstrap": "^0.27.1",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "*",
    "redux": "^3.0.2",
    "redux-devtools": "^2.1.5",
    "redux-thunk": "^1.0.0",
    "rimraf": "^2.4.3",
    "style-loader": "*",
    "webpack": "^1.12.2",
    "webpack-target-electron-renderer": "^0.1.0"
  },
  "babel": {
    "stage": 0
  }
}
