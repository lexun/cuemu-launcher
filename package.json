{
  "name": "swg-launcher",
  "version": "1.0.0",
  "description": "Patcher and Launcher for SWG",
  "main": "build/main.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "clean": "./node_modules/.bin/rimraf build",
    "dev": "./node_modules/.bin/webpack --progress --colors --watch",
