{
  "name": "cuemu-launcher",
  "version": "2.0.0-alpha.1",
  "description": "SWG launcher for CUEmu",
  "productName": "Launcher",
  "homepage": "https://github.com/lexun/cuemu-launcher#readme",
  "author": "Luke Barbuto",
  "license": "MIT",
  "main": "./out/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lexun/cuemu-launcher.git"
  },
  "bugs": {
    "url": "https://github.com/lexun/cuemu-launcher/issues"
  },
  "scripts": {
    "compile": "npm run clean && NODE_ENV=production webpack",
    "clean": "rimraf out",
    "dev-server": "NODE_ENV=development webpack-dev-server",
    "dist": "npm run compile && build -w --x64",
    "pack": "npm run compile && build",
    "postinstall": "install-app-deps",
    "release": "build -w --x64",
    "start-dev": "NODE_ENV=development electron ./out",
    "start": "NODE_ENV=production electron ./out"
  },
  "build": {
    "appId": "com.cuemu.launcher",
    "files": [
      "out",
      "node_modules"
    ],
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "iconUrl": "https://dl.dropboxusercontent.com/u/17420787/cuemu/cuemu.ico"
    }
  },
  "dependencies": {
    "electron-log": "^1.3.0",
    "electron-updater": "^1.4.2"
  },
  "devDependencies": {
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/extract-text2": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "electron-builder": "^12.0.3",
    "electron": "^1.4.15",
    "elm-css-webpack-loader": "^2.0.2",
    "elm-hot-loader": "^0.5.4",
    "elm-webpack-loader": "^4.2.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.26.0",
    "rimraf": "^2.5.4",
    "webpack-dev-server": "^2.2.0",
    "webpack": "^2.2.0"
  }
}
